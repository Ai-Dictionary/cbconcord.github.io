        background-repeat: no-repeat;
        background-size: cover;
        background-position: 50% 50%;
        padding: 0px;
        margin: 15px 1%;
        img{
            display: none;
        }
    }
    @media only screen and (max-width: 900px), only screen and (min-width: 900px) and (max-width: 1200px){
