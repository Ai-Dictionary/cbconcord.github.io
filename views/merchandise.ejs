<style>
    .dis-none {
        display: none;
    }
    .flx {
        display: flex;
        flex-wrap: wrap;
        justify-content: left;
    }
    .flx .product {
        width: 30%;
        height: 340px;
        margin: 40px 1%;
        background-color: white;
        .head{
            width: 100%;
            height: 80%;
            img{
                position: relative;
                width: 100%;
                height: 100%;
            }
        }
        .title{
            width: 100%;
            height: 20%;
            text-align: center;
            font-size: 18px;
            font-family: cursive;
            color: orange;
            padding-top: 20px;
            overflow: hidden;
        }
    }
    @media only screen and (max-width: 900px), only screen and (min-width: 900px) and (max-width: 1200px) {
        .flx .product {
            width: 90%;
            margin: 40px auto;
            .title{
                font-size: 16px;
                padding-top: 0px;
            }
        }
    }
</style>
<body class="defult-home">
    <div id="loader" class="loader">
        <div class="loader-container"></div>
    </div>

    <span id="header">
        <%= header %>
    </span>

    <!-- Merchandise Section Start -->
    <div class="rs-services main-home pt-120 pb-120 md-pt-70 md-pb-70" style="padding-top: 30px!important;">
        <div class="container">
            <div class="events">
                <div class="sec-title2 text-center mb-45">
                    <h2 class="title">
                        Our Merchandise
                    </h2>
                    <div class="heading-line"></div>
                    <div class="flx">
                        <div class="product">
                            <div class="head">
                                <img src="../images/Merchandise/1.jpeg" alt="cbsconcord"/>
                            </div>
                            <div class="title">Concord 2025 Badge</div>
                        </div>
                        <div class="product">
                            <div class="head">
                                <img src="../images/Merchandise/2.jpeg" alt="cbsconcord"/>
                            </div>
                            <div class="title">Concord 2025 Limited Edition Band</div>
                        </div>
                        <div class="product">
                            <div class="head">
                                <img src="../images/Merchandise/3.jpeg" alt="cbsconcord"/>
                            </div>
                            <div class="title">Concord 2025 White and Black Bands</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Merchandise Section End -->

