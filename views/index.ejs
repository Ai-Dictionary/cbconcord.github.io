<style>
    @font-face {
        font-family: 'Old English 5';
        src: url('../assets/fonts/OldEnglishFive-axyVg.ttf') format('truetype');
    }
    .banner{
        display: flex;
        flex-direction: row;
        justify-content: space-around;
        width: 100%;
        margin: 50px auto 20px auto;
        user-select: none;
        .left-banner, .right-banner{
            width: 50%;
        }
    }
    .left-banner{
        padding-top: 2%;
        text-align: center;
        h2{
            font-family: "Old English 5", Sans-serif;
        }
        h1{
            font-family: "Old English 5", Sans-serif;
            background: linear-gradient(50deg, #FF6700 30%, #f3c242 50%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            font-size: 70px;
        }
        img{
            width: 40%;
        }
        .social{
            width: 70%;
            display: flex;
            flex-direction: row;
            margin: 10px auto;
            .social_icon{
                width: 35px;
                height: 35px;
                margin-right: 12px;
                background: #f3c242;
                border-radius: 50%;
                cursor: pointer;
                i{
                    font-size: 18px;
                    margin: 8px auto;
                }
            }
            .social_icon:hover{
                background: #fff;
                color: #f3c242;
            }
        }
    }
    .right-banner{
        padding: 10px 0px;
        text-align: center;
        color: #f3c242;
        font-size: 22px;
        font-family: "Old English 5", Sans-serif;
        iframe{
            width: 90%;
            height: 90%;
            margin: 2% 5%;
        }
    }
    .crop-image{
        height: 214px;
        max-width: 30%;
        background: grey;
        background-repeat: no-repeat;
        background-size: cover;
        background-position: 50% 50%;
        padding: 0px;
        margin: 15px 1%;
        img{
            display: none;
        }
    }
    .project-img {
        width: 100%!important;
        height: 350px!important;
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center;
    }
    .elementor-tab-content li a{
        color: slategray!important;
    }
    @media only screen and (max-width: 900px){
        /* , only screen and (min-width: 900px) and (max-width: 1200px){ */
        .banner{
            flex-direction: column-reverse;
            .left-banner, .right-banner{
                width: 100%;
            }
        }
        .crop-image {
            height: 198px;
            max-width: 95%;
            margin: 15px auto;
        }
        .left-banner{
            h2{
                font-size: 22px;
            }
            h1{
                font-size: 40px;
            }
            img{
                width: 70%;
            }
        }
    }
</style>
