<script>
    document.getElementById('header').innerHTML = document.getElementById('header').textContent;
    document.getElementById('rs-footer').innerHTML = document.getElementById('rs-footer').textContent;
    document.getElementById('eventsSchedule').innerHTML = document.getElementById('eventsSchedule').textContent;

    function setGallary(){
        const lazyBackgrounds = document.querySelectorAll('.crop-image');
        const observer = new IntersectionObserver((entries) => {
            entries.forEach((entry) => {
                if (entry.isIntersecting) {
                    const backgroundImage = entry.target.getAttribute('data-background-image');
                    entry.target.style.backgroundImage = `url(${backgroundImage.replaceAll(' ','%20')})`;
                    observer.unobserve(entry.target);
                }
            });
        }, {
            rootMargin: '50px',
        });

        lazyBackgrounds.forEach((lazyBackground) => {
            observer.observe(lazyBackground);
        });
    }
    setTimeout(()=>{
        setGallary();
    },1000);
    function setEvents(){
        let box = document.querySelectorAll('.project-img');
        for(let i=0; i<box.length; i++){
            box[i].style.backgroundImage = `url('${box[i].childNodes[1].childNodes[0].src}')`;
            box[i].childNodes[1].childNodes[0].style.display = "none";
        }
    }
    setEvents();
</script>
